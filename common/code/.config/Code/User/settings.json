{
    "telemetry.enableTelemetry": false,
    "workbench.startupEditor": "newUntitledFile",
    "window.menuBarVisibility": "toggle",
    "editor.fontFamily": "'DroidSansMono Nerd Font Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "git.autofetch": true,
    "git.confirmSync": false,
    "update.mode": "manual",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    }
}
